{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "fasm",
      "camelcase": "Fasm",
      "title": "FASM",
      "scope": "source.fasm",
      "file-types": [
        "fasm",
        "asm",
        "inc"
      ],
      "injection-regex": "^fasm$",
      "class-name": "TreeSitterFasm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Fasm grammar for tree-sitter",
    "authors": [
      {
        "name": "Amaan Qureshi",
        "email": "contact@amaanq.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-fasm"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
